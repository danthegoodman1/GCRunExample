gcloud builds submit --tag gcr.io/[PROJECT-ID]/helloworld --project=PROJECT(if project not set in gcloud cli)

gcloud beta run deploy --image gcr.io/[PROJECT-ID]/helloworld --project=[PROJECT] --update-env-vars KEY1=VALUE1,KEY2=VALUE2

gcloud beta run services update [SERVICE] --project=[PROJECT] --update-env-vars KEY1=VALUE1,KEY2=VALUE2
